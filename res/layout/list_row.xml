<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/AbsoluteLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f6f8fa"
    android:clickable="true" >

    <com.parse.ParseImageView
        android:id="@+id/imageView3"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_x="15dp"
        android:layout_y="15dp"
        android:src="@drawable/inspiration_6" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_x="276dp"
        android:layout_y="6dp"
        android:src="@drawable/time" />

    <TextView
        android:id="@+id/releaseYear"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_x="300dp"
        android:layout_y="7dp"
        android:textColor="#34BcA5"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="133dp"
        android:layout_y="14dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="73dp"
        android:layout_y="13dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="12sp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_x="9dp"
        android:layout_y="9dp"
        android:src="@drawable/post_st2" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="17dp"
        android:layout_height="17dp"
        android:layout_x="329dp"
        android:layout_y="45dp"
        android:src="@drawable/replay" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:layout_x="328dp"
        android:layout_y="65dp"
        android:src="@drawable/repost" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="15dp"
        android:layout_height="23dp"
        android:layout_x="330dp"
        android:layout_y="88dp"
        android:clickable="true"
        android:src="@drawable/delete" />

     <RelativeLayout
         android:id="@+id/RL"
	     android:layout_width="match_parent"
	     android:layout_height="match_parent">

         <TextView
             android:id="@+id/textView1"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignParentTop="true"
             android:layout_centerHorizontal="true"
             android:layout_marginTop="58dp"
             android:gravity="center"
             android:paddingBottom="10dp"
             android:paddingLeft="50dp"
             android:paddingRight="50dp"
             android:text="TextView" />

         <com.parse.ParseImageView
             android:id="@+id/icon"
             android:layout_width="100dp"
             android:layout_height="100dp"
             android:layout_below="@+id/textView1"
             android:layout_centerHorizontal="true"
             android:layout_marginTop="15dp" />

     </RelativeLayout>
     
    

    <Button
        android:id="@+id/button1"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="27dp"
        android:layout_height="24dp"
        android:layout_x="325dp"
        android:layout_y="117dp"
        android:background="@drawable/follow" />
        
</AbsoluteLayout>